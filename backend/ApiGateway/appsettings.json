{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "dev-p44mo426qxs1apjs.us.auth0.com",
    "Audience": "https://api.stocktok.com"
  },
  "ReverseProxy": {
    "Routes": {
      "news-route": {
        "ClusterId": "news-cluster",
        "Match": {
          "Path": "/api/News/{**catch-all}"
        }
      },
      "posts-route": {
        "ClusterId": "posts-cluster",
        "Match": {
          "Path": "/api/Posts/{**catch-all}"
        }
      },
      "posts-hub-route": {
        "ClusterId": "posts-cluster",
        "Match": {
          "Path": "/hubs/comments/{**catch-all}"
        }
      }
    },
    "Clusters":{
      "news-cluster": {
        "Destinations": {
          "news-service": {
            "Address": "http://localhost:5214"
          }
        }
      },
      "posts-cluster": {
        "Destinations": {
          "posts-service": {
            "Address": "http://localhost:5214"
          }
        }
      }
    }
  }
}
